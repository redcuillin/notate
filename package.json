    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "jest --coverage",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:CI": "start-server-and-test start http://localhost:3000 cypress:run",
    "lint": "NODE_PATH=src eslint --ext .js,.jsx src",
    "lint:fix": "yarn lint --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run test",
      "pre-push": "npm run cypress:CI"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "engines": {
    "node": "16.16.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "aws-sdk": "^2.1257.0",
    "cypress": "^11.1.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "glob": "^8.0.3",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "mime-types": "^2.1.35",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "start-server-and-test": "^1.14.0"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>test/setup/onFailPropType.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>test/setup/setupEnzyme.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/test/setup/",
      "<rootDir>/cypress"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
